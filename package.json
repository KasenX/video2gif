{
  "name": "video2gif-server",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/src/server.js",
    "dev": "node --env-file=.env dist/src/server.js",
    "build": "tsc && npm run copy-public && npm run tailwind && npm run create-dirs",
    "copy-public": "cp -r ./public ./dist",
    "tailwind": "npx tailwindcss -i public/style.css -o dist/public/style.css",
    "create-dirs": "mkdir -p -m 777 dist/videos dist/gifs",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KasenX/video2gif.git"
  },
  "author": "Jakub Janecek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KasenX/video2gif/issues"
  },
  "homepage": "https://github.com/KasenX/video2gif#readme",
  "description": "",
  "dependencies": {
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-fileupload": "^1.5.1",
    "fluent-ffmpeg": "^2.1.3",
    "jsonwebtoken": "^9.0.2",
    "kysely": "^0.27.4",
    "pg": "^8.12.0",
    "tslib": "^2.6.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/express-fileupload": "^1.5.0",
    "@types/fluent-ffmpeg": "^2.1.25",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.1.0",
    "@types/pg": "^8.11.6",
    "@types/uuid": "^10.0.0",
    "globals": "^15.9.0",
    "tailwindcss": "^3.4.10",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.4"
  }
}
